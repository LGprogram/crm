        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kaishengit.mapper.RoleMapper">


    <select id="findById" resultType="com.kaishengit.pojo.Role">
        select * from t_role where id= #{id}
